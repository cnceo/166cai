<script>
	$(function(){
		var goTop = $("#topGo")
		var timer = null;
		var gotopShow = function(){
			var scrollTop = $(window).scrollTop();
			if(scrollTop > 566){
				goTop.fadeIn(400);
			}else{
				goTop.fadeOut(400);
			}
		}
		
		$(window).on('scroll', function(){
			goTop.hide();
			clearTimeout(timer);
			timer = setTimeout(gotopShow, 400)
		});
		$("#topGo").on('click', function() {
			$("body,html").animate({
				scrollTop: 0
			}, 400);
		});
	})
</script>
